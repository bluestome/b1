        <meta-data
            android:name="isDebug"
            android:value="true" >
        </meta-data>
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="4f9a31d45270156a8e000029" >
        </meta-data>

        <!-- must be one line -->
        <meta-data  android:name="UMENG_CHANNEL" android:value="5000" /> 
        <meta-data  android:name="SKYMOBI_CHANNEL" android:value="5000" />
    </application>

</manifest>